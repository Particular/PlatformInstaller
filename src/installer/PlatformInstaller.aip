    <ROW Type="1" DisplayString="Uninstall_Apps" Note="Uninstalls/removes resources from the machine based on the user&apos;s selection in the installer dialog. &#13;&#10;&#13;&#10;If nothing is scheduled to be removed the custom action will exit immediately." TargetMsiRow="Uninstall_Apps" TableName="InstallUISequence"/>
    <ROW Type="1" DisplayString="Install_Transport_NSB" Note="Installs selected transport type for NSB, eg. &quot;MSMQ&quot;" TargetMsiRow="Install_Transport_NSB" TableName="InstallUISequence"/>
    <ROW Type="1" DisplayString="DownloadHelperDll_NSB.Powershell" Note="Downloads the DLL NServiceBus.Powershell.dll to be used for installing the selected user transport.&#13;&#10;&#13;&#10;We do not include it in the package because its about 8 MB large, it would practically double our package size." TargetMsiRow="DownloadHelperDll_NSB.Powershell" TableName="InstallUISequence"/>
